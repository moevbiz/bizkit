{
  "name": "bizkit",
  "version": "1.0.0",
  "description": "Kirby is a file-based CMS. Easy to setup. Easy to use. Flexible as hell.",
  "main": "index.js",
  "config": {
    "webpack": "node_modules/laravel-mix/setup/webpack.config.js"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "babel-loader": "^8.1.0",
    "browser-sync": "^2.26.14",
    "browser-sync-webpack-plugin": "^2.2.2",
    "css-loader": "^5.0.1",
    "file-loader": "^6.2.0",
    "laravel-mix": "^6.0.13",
    "mini-css-extract-plugin": "^1.3.3",
    "sass": "^1.32.4",
    "sass-loader": "^10.1.1",
    "terser-webpack-plugin": "^5.1.1",
    "url-loader": "^4.1.1",
    "webpack": "^5.13.0",
    "webpack-cli": "^4.2.0",
    "webpack-merge": "^5.3.0"
  },
  "scripts": {
    "dev": "webpack --config webpack.config.js --mode=development --watch",
    "mix-dev": "webpack --watch --progress --config=$npm_package_config_webpack --mode=development",
    "build": "webpack --config webpack.config.js",
    "push": "git add . && git commit -m \"$(date +\"%Y-%m-%d %T\")\" && git push"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/moevbiz/bizkit.git"
